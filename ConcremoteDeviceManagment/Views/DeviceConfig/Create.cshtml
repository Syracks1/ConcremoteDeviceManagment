@model ConcremoteDeviceManagment.Models.DeviceConfig

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>DeviceConfig</h4>
        <hr />
    </div>
}

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <form method="POST" action="DeviceSteps">
            <input type="text" name="createAmount" placeholder="Amount" class="form-control" /><br /><br />
            <input type="submit" value="Submit" class="btn btn-default" />
        </form>
    </div>
    @Html.DisplayFor(model => model.DeviceType.name)<br />
</div>

<div>
    <button onclick="location.href='@Url.Action("Index", "DeviceConfig")';return false; " class="btn btn-primary">Back to list</button>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}